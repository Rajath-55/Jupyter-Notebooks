# Preferred editor for local and remote sessions
# if [[ -n $SSH_CONNECTION ]]; then
#   export EDITOR='vim'
# else
#   export EDITOR='mvim'
# fi
function acp() {
    git add .
    if [ "$1" != "" ]
    then
        git commit -m "$1"
    else
        git commit -m update
    fi
    git push origin master
}

function repo(){
    git init 
    git add .
    if [ "$1" != "" ]
    then 
        git remote add origin $1 
        git commit -m "First Add"
        git push origin master
    fi 
}

function check(){
    if [ "$1" != "" ]
    then 
        g++ "$1" 
        cat input.txt | ./a.out > output.txt 
        echo "Input file found , output being generated : "
        echo "Output file made : "
        cat output.txt 
    else
        echo "Filename not provided"
    fi 
}

function leet(){
    if [ "$1" != "" ]
    then
        clipboard | cat > "$1".cpp 
    else
        echo "Filename not provided"
    fi

}

function vcpp(){
    if [ "$1" != "" ]
    then 
        cat /Users/rajathv/Desktop/stuff/smalltemplate.cpp | clipboard 
        clipboard | cat > "$1".cpp 
        nvim "$1".cpp  
    else
        echo "Filename not provided"
    fi
}

function cmpl(){
    if [ "$1" != "" ]
    then
        g++ -std=c++14 "$1".cpp
        touch output.txt
        cat input.txt | ./a.out > output.txt
    else
        echo "Filename not provided"
    fi
}


